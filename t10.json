{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "AU",
  "properties": {
    "M1": {
      "type": "string"
    },
    "jsonList": {
      "type": "array",
      "items": {
        "type": "object",
        "title": "JsonOriginListItem",
        "properties": {
          "A1": {
            "type": "string"
          },
          "opdata": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string",
                  "examples": ["ni","n2"],
                  "description":"aaaaaa"
            
                },
                "value": {
                  "type": "string"
                }
              },
              "required": ["name", "value"]
            }
          }
        },
        "required": ["A1"]
      }
    },
    "opdatalist": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "$comment": "n1,n4"
          },
          "value": {
            "type": "string"
          }
        },
        "required": ["name", "value"]
      }
    }
  },
  "required": ["M1", "jsonList"]
}
